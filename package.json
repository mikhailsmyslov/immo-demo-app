{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@reduxjs/toolkit": "^1.4.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.15.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "fontsource-roboto": "^2.2.6",
    "formik": "^2.1.5",
    "formik-material-ui": "^3.0.0-alpha.0",
    "i18next": "^19.6.3",
    "i18next-browser-languagedetector": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash-es": "^4.17.15",
    "newsapi": "^2.4.1",
    "node-sass": "^4.14.1",
    "notistack": "^0.9.17",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "typescript": "^3.7.5",
    "yup": "^0.29.2"
  },
  "scripts": {
    "start-frontend:dev": "react-scripts start",
    "build-frontend": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-backend:dev": "npx nodemon --exec npx babel-node server/bin/app.js",
    "build-backend": "babel server --out-dir dist --source-maps inline",
    "build": "NODE_ENV=production npm run build-frontend && npm run build-backend",
    "start": "dist/bin/app.js",
    "start:dev": "NODE_ENV=development concurrently \"npm run start-frontend:dev\" \"npm run start-backend:dev\"",
    "lint": "npx eslint . --ext js,jsx,ts,tsx --fix",
    "postinstall": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/!node_modules\\/lodash-es/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/node": "^7.10.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/classnames": "^2.2.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.7",
    "@types/jest": "^24.9.1",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.12.53",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/yup": "^0.29.3",
    "concurrently": "^5.2.0",
    "http-proxy-middleware": "^1.0.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
